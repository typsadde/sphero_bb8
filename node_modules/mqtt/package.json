    "nsp": "^3.2.1",
    "pre-commit": "^1.2.2",
    "rimraf": "^2.6.2",
    "safe-buffer": "^5.1.2",
    "should": "^13.2.1",
    "sinon": "~1.17.7",
    "snazzy": "^7.1.1",
    "standard": "^11.0.1",
    "through2": "^2.0.3",
    "tslint": "^5.10.0",
    "tslint-config-standard": "^7.0.0",
    "typescript": "^2.8.3",
    "uglify-js": "^3.3.24",
    "ws": "^3.3.3",
    "zuul": "^3.11.1",
    "zuul-ngrok": "^4.0.0"
  },
  "standard": {
    "env": [
      "mocha"
    ]
  },
